    (function () {
        const
            request = new XMLHttpRequest(),
            contextPath = document.querySelector('meta[name="ajs-context-path"]').getAttribute('content');
        request.open("GET", contextPath + "/rest/jeditortemplateresource/1.0/projectlevel/cached/templates?t=null", true);

        request.onload = function ()
        {
            if (request.status >= 200
                && request.status < 400)
            {
                let data;

                try {
                    data = JSON.parse(request.responseText);
                } catch (exception) {
                    console.error('JEDITOR: failed to parse project-level templates');
                    data = null;
                }

                if (data) {
                    if (window.JEDITOR && JEDITOR.addTemplates) {
                        JEDITOR.addTemplates('jeditor_project_templates', data);
                        console.log('%cJEDITOR: templates loaded', 'background: #333; color: #4c9aff');
                    }
                    else {
                        let i = 0,
                            max = 300; // max 30 sec
                        (function loop() {
                            setTimeout(function() {
                                if (i > max) {
                                    return;
                                }
                                if (window.JEDITOR && JEDITOR.addTemplates) {
                                    JEDITOR.addTemplates('jeditor_project_templates', data);
                                    console.log('%cJEDITOR: templates loaded', 'background: #333; color: #4c9aff');
                                }
                                else {
                                    loop();
                                    i++;
                                }
                            }, 100);
                        })();
                    }
                }
            }
            else if (request.status === 401 || request.status === 403) {
                // do nothing
            }
            else {
                console.error('JEDITOR: failed to fetch project-level templates from the server');
                console.error('JEDITOR: network request failed with status ' + request.status);
                logResponse(request);
            }
        };

        request.onerror = function () {
            console.error('JEDITOR: a network error prevented fetching project-level templates from the server');
        };

        request.send();

        function logResponse(request) {
            const response = request.responseText || request.statusText;
            if (response && response.length && response.length < 1000) {
                console.error(response);
            }
        }
    })();
</script><script type="text/javascript">var AJS=AJS||{};AJS.debug=true;</script>


    
